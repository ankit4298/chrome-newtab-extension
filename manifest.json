{
	"manifest_version": 3,
	"name": "Customizable new tab",
	"version": "0.5",
	"description": "Customizable new tab",
	"author": "Ankit Patel",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"options_ui": {
		"page": "/options/options.html",
		"open_in_tab": false
	},
	"icons": {
		"16": "/icons/icon16.png",
		"48": "/icons/icon64.png",
		"128": "/icons/icon128.png"
	},
	"permissions": [
		"storage"
	],
	"host_permissions": [
		"https://picsum.photos/"
	]
}